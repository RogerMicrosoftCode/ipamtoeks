# APIM Configuration
APIM_RESOURCE_GROUP=your-apim-resource-group
APIM_SERVICE_NAME=your-apim-service-name
APIM_SUBSCRIPTION_ID=your-subscription-id
APIM_API_ID=your-api-id
APIM_PRODUCT_ID=your-product-id

# EKS Configuration
EKS_CLUSTER_NAME=your-eks-cluster-name
EKS_REGION=us-west-2
EKS_NAMESPACE=default
AWS_ACCOUNT_ID=your-aws-account-id

# Token Configuration
TOKEN_SECRET_NAME=apim-eks-token
TOKEN_EXPIRY_DAYS=90
TOKEN_ROTATION_ENABLED=true

# Deployment Configuration
DEPLOYMENT_NAME=apim-eks-integration
CONTAINER_REGISTRY=your-registry.azurecr.io
IMAGE_NAME=apim-connector
IMAGE_TAG=latest

# Pipeline Configuration
ENABLE_AUTO_DEPLOY=true
ENABLE_ROLLBACK=true
HEALTH_CHECK_INTERVAL=30
